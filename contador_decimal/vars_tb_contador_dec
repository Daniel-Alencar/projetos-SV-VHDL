#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
:vpi_module "v2009";
S_0x557d9757b9f0 .scope module, "tb_contador_dec" "tb_contador_dec" 2 7;
 .timescale -9 -9;
v0x557d9758b240_0 .var "clk", 0 0;
v0x557d9758b2e0_0 .net "cnt", 3 0, v0x557d9758b100_0;  1 drivers
S_0x557d97545680 .scope module, "UUT" "contador_dec" 2 16, 3 1 0, S_0x557d9757b9f0;
 .timescale -9 -9;
    .port_info 0 /INPUT 1 "CLK"
    .port_info 1 /OUTPUT 4 "CNT"
v0x557d97545870_0 .net "CLK", 0 0, v0x557d9758b240_0;  1 drivers
v0x557d9758b100_0 .var "CNT", 3 0;
E_0x557d97545ca0 .event posedge, v0x557d97545870_0;
    .scope S_0x557d97545680;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x557d9758b100_0, 0, 4;
    %end;
    .thread T_0, $init;
    .scope S_0x557d97545680;
T_1 ;
    %wait E_0x557d97545ca0;
    %load/vec4 v0x557d9758b100_0;
    %addi 1, 0, 4;
    %store/vec4 v0x557d9758b100_0, 0, 4;
    %load/vec4 v0x557d9758b100_0;
    %pad/u 32;
    %cmpi/e 10, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x557d9758b100_0, 0, 4;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x557d9757b9f0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x557d9758b240_0, 0, 1;
    %end;
    .thread T_2, $init;
    .scope S_0x557d9757b9f0;
T_3 ;
    %vpi_call/w 2 24 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 2 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x557d9757b9f0 {0 0 0};
T_3.0 ;
    %delay 1, 0;
    %load/vec4 v0x557d9758b240_0;
    %inv;
    %store/vec4 v0x557d9758b240_0, 0, 1;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x557d9757b9f0;
T_4 ;
    %delay 60, 0;
    %vpi_call/w 2 34 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_contador_dec.sv";
    "./contador_dec.sv";
