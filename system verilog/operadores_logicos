#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
:vpi_module "v2009";
S_0x563382da1620 .scope module, "tb_operadores" "tb_operadores" 2 1;
 .timescale 0 0;
v0x563382da17a0_0 .var "a", 3 0;
v0x563382de6970_0 .var "b", 3 0;
v0x563382de6a50_0 .var "c", 3 0;
    .scope S_0x563382da1620;
T_0 ;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x563382da17a0_0, 0, 4;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x563382de6970_0, 0, 4;
    %load/vec4 v0x563382de6970_0;
    %load/vec4 v0x563382da17a0_0;
    %cmp/u;
    %flag_get/vec4 5;
    %load/vec4 v0x563382de6a50_0;
    %load/vec4 v0x563382de6970_0;
    %cmp/u;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %vpi_call/w 2 10 "$display", "Teste 1 Verdadeiro" {0 0 0};
T_0.0 ;
    %load/vec4 v0x563382de6970_0;
    %load/vec4 v0x563382da17a0_0;
    %cmp/u;
    %flag_mov 8, 5;
    %load/vec4 v0x563382de6a50_0;
    %load/vec4 v0x563382de6970_0;
    %cmp/u;
    %flag_or 5, 8;
    %jmp/0xz  T_0.2, 5;
    %vpi_call/w 2 13 "$display", "Teste 2 Verdadeiro" {0 0 0};
T_0.2 ;
    %load/vec4 v0x563382de6970_0;
    %load/vec4 v0x563382da17a0_0;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz  T_0.4, 5;
    %vpi_call/w 2 16 "$display", "Teste 3 Verdadeiro" {0 0 0};
T_0.4 ;
    %load/vec4 v0x563382da17a0_0;
    %load/vec4 v0x563382de6970_0;
    %concat/vec4; draw_concat_vec4
    %vpi_call/w 2 18 "$display", "Concatenacao de a e b %b", S<0,vec4,u8> {1 0 0};
    %vpi_call/w 2 19 "$display", "Concatenacao e repeticao de padrao %b", 8'b01010101 {0 0 0};
    %vpi_call/w 2 21 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "operadores_logicos.sv";
