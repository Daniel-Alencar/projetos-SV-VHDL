#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
:vpi_module "v2009";
S_0x559921f50910 .scope module, "tb_operadores" "tb_operadores" 2 1;
 .timescale 0 0;
v0x559921f50a90_0 .var "a", 3 0;
v0x559921f84880_0 .var "b", 3 0;
    .scope S_0x559921f50910;
T_0 ;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x559921f50a90_0, 0, 4;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x559921f84880_0, 0, 4;
    %load/vec4 v0x559921f50a90_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %vpi_call/w 2 10 "$display", "Valor do deslocamento a direita: %b", S<0,vec4,u4> {1 0 0};
    %load/vec4 v0x559921f50a90_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %vpi_call/w 2 11 "$display", "Valor do deslocamento a esquerda: %b", S<0,vec4,u4> {1 0 0};
    %load/vec4 v0x559921f50a90_0;
    %inv;
    %vpi_call/w 2 12 "$display", "Valor do complemento de a: %b", S<0,vec4,u4> {1 0 0};
    %load/vec4 v0x559921f50a90_0;
    %load/vec4 v0x559921f84880_0;
    %and;
    %vpi_call/w 2 13 "$display", "Valor a and b: %b", S<0,vec4,u4> {1 0 0};
    %load/vec4 v0x559921f50a90_0;
    %load/vec4 v0x559921f84880_0;
    %or;
    %vpi_call/w 2 14 "$display", "Valor a or b: %b", S<0,vec4,u4> {1 0 0};
    %load/vec4 v0x559921f50a90_0;
    %load/vec4 v0x559921f84880_0;
    %xor;
    %vpi_call/w 2 15 "$display", "Valor a xor b: %b", S<0,vec4,u4> {1 0 0};
    %vpi_call/w 2 17 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "outros_operadores.sv";
